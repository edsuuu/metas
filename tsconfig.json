{
  "compilerOptions": {
    "allowJs": true,
    "module": "ESNext",
    "target": "esnext",
    "moduleResolution": "Bundler",
    "jsx": "react-jsx",
    "strict": true,
    "sourceMap": true,
    "skipLibCheck": true,
    "noImplicitAny": false,
    "paths": {
      "@/*": ["./resources/js/*"]
    },
    "types": ["vite/client", "node"]
  },
  "include": ["resources/js/**/*", "resources/js/**/*.vue"],
  "exclude": ["node_modules", "public"]
}
